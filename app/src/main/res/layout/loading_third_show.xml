<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/image_welcome_three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/welcome_three"
        android:layout_marginTop="@dimen/ui_150"
        android:layout_gravity="center_horizontal"/>

    <TextView
        android:id="@+id/tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/ui_30"
        android:textStyle="bold"
        android:text="预约功能"
        android:textSize="@dimen/font_50"
        android:textColor="@color/color_FF6F4A"/>

    <TextView
        android:id="@+id/tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/ui_margin_20"
        android:text="预约功能即将上线！"
        android:textSize="@dimen/font_26"
        android:textColor="@color/color_FF6F4A"/>

    <TextView
        android:id="@+id/image_into"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/ui_60"
        android:gravity="center"
        android:text="点击进入"
        android:textSize="@dimen/font_30"
        android:textColor="@color/color_FF6F4A"
        android:background="@drawable/welcome_into_bg" />

</LinearLayout>